{
  "name": "koa-photo-api",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "clean": "rimraf lib/*",
    "precompile": "npm run clean",
    "compile": "babel src -d lib",
    "test-no-compile": "mocha --compilers js:babel-register ./test -u bdd -R spec",
    "test": "npm run compile && npm run test-no-compile",
    "test:debug": "npm run compile && concurrently \"npm run test-no-compile -- --debug-brk\" \"node-inspector\"",
    "watch:src": "watch \"npm run compile && npm run test-no-compile\" ./src",
    "watch:test": "watch \"npm run test-no-compile\" ./test",
    "watch": "concurrently \"npm run watch:test\" \"npm run watch:src\"",
    "start:debug": "nodemon --debug lib/app.js",
    "start": "node lib/app.js",
    "debug": "concurrently \"npm run start:debug\" \"npm run watch\" \"node-inspector\""
  },
  "repository": {
    "type": "git",
    "url": "ssh://gituser@192.168.1.225:60022/volume1/code/git/koa-photo-api.git"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-to-generator"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "concurrently": "^2.2.0",
    "mocha": "^2.5.3",
    "node-inspector": "^0.12.8",
    "nodemon": "^1.9.2",
    "rimraf": "^2.5.3",
    "supertest": "^1.2.0",
    "watch": "^0.19.1"
  },
  "dependencies": {
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.1.0",
    "koa-simple-router": "^0.2.0"
  }
}
